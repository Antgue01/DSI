    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <TextBlock HorizontalAlignment="Left" Text="Page 2" TextWrapping="Wrap" VerticalAlignment="Top" Margin="10,42,0,0"/>
